<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item>
        <shape android:shape="rectangle">
            <size android:width="50dp" android:height="50dp"/>
            <corners android:radius="5dp"/>
            <stroke android:width="2dp" android:color="#AB878080"/>
        </shape>
    </item>
    <item android:bottom="20dp">
        <shape android:shape="line">
            <stroke android:width="2dp" android:color="#AB878080"/>
        </shape>
    </item>
    <item android:right="10dp" android:left="15dp" android:bottom="-10dp">
        <shape android:shape="line">
            <stroke android:width="1dp" android:color="#AB878080"/>
        </shape>
    </item>
    <item android:right="10dp" android:left="15dp" >
        <shape android:shape="line">
            <stroke android:width="1dp" android:color="#AB878080"/>
        </shape>
    </item>
    <item android:right="10dp" android:left="15dp" android:bottom="-20dp">
        <shape android:shape="line">
            <stroke android:width="1dp" android:color="#AB878080"/>
        </shape>
    </item>

</layer-list>